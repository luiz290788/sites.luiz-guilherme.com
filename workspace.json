{
  "version": 1,
  "projects": {
    "personal": {
      "root": "apps/personal",
      "sourceRoot": "apps/personal/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/personal/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "builder": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "personal:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "personal:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/personal/tsconfig.json",
              "apps/personal/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/personal/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/personal/jest.config.js",
            "tsConfig": "apps/personal/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "personal-e2e": {
      "root": "apps/personal-e2e",
      "sourceRoot": "apps/personal-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/personal-e2e/cypress.json",
            "tsConfig": "apps/personal-e2e/tsconfig.e2e.json",
            "devServerTarget": "personal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "personal:serve:production"
            },
            "watch": {
              "watch": true
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/personal-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/personal-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/gatsby": {
      "application": {}
    }
  },
  "defaultProject": "personal"
}
